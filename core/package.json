{
  "name": "mintbean-cli",
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "mint": "bin/mint"
  },
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "mint": "tsdx build && node ./dist/index.js",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "kmbarnett",
  "module": "dist/core.esm.js",
  "size-limit": [
    {
      "path": "dist/core.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/core.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "4.6.0",
    "@types/esm": "3.2.0",
    "@types/fs-extra": "9.0.1",
    "@types/glob": "7.1.3",
    "@types/klaw": "3.0.1",
    "@types/lodash": "4.14.161",
    "@types/yargs": "15.0.7",
    "husky": "4.3.0",
    "size-limit": "4.6.0",
    "tsdx": "0.14.0",
    "tslib": "2.0.1"
  },
  "dependencies": {
    "dotpref": "1.0.0",
    "fs-extra": "9.0.1",
    "glob": "7.1.6",
    "klaw": "3.0.0",
    "lodash": "4.17.20",
    "path": "0.12.7",
    "yargs": "16.0.3"
  }
}
